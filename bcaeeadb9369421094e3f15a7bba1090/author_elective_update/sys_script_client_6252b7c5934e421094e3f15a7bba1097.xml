    <sys_script_client action="DELETE">
        <active>true</active>
        <applies_extended>false</applies_extended>
        <condition/>
        <description/>
        <field>start_date</field>
        <global>true</global>
        <isolate_script>true</isolate_script>
        <messages/>
        <name>Item start date validation</name>
        <order/>
        <script><![CDATA[function onChange(control, oldValue, newValue, isLoading, isTemplate) {
    if (isLoading || newValue === '') {
        return;
    }

    var isNewRecord = g_form.isNewRecord();

    var startDate = g_form.getValue('start_date');
    var currentDate = new Date();

    var startDateObj = new Date(startDate);

    if (startDateObj.toDateString() === currentDate.toDateString()) {
        g_form.clearValue('start_date');
        g_form.showFieldMsg('start_date', getMessage('Start date cannot be today.'), 'error');
        return;
    } else if (isNewRecord && startDateObj < currentDate) {
        g_form.clearValue('start_date');
        g_form.showFieldMsg('start_date', getMessage('Start date cannot be before today.'), 'error');
        return;
    }

    g_form.clearMessages('start_date');
}
]]></script>
