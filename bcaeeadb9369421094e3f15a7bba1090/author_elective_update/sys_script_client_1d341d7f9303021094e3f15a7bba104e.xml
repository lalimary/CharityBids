        <name>duration count</name>
        <order/>
        <script><![CDATA[function onChange(control, oldValue, newValue, isLoading, isTemplate) {
   if (isLoading || newValue === '') {
      return;
   }

    // Get the current date
    var currentDate = new Date();

    // Get the start date and end date from the form
    var startDateStr = g_form.getValue('start_date');
    var endDateStr = g_form.getValue('end_date');

    if (!startDateStr || !endDateStr) {
        return; // Exit if start date or end date is not set
    }

    var startDate = new Date(startDateStr);
    var endDate = new Date(endDateStr);

    if (currentDate < startDate) {
        // Calculate days to start date
        var daysToStart = dateDiffInDays(currentDate, startDate);
        g_form.setValue('duration', daysToStart);
    } else if (currentDate >= startDate && currentDate <= endDate) {
        // Calculate days to end date
        var daysToEnd = dateDiffInDays(currentDate, endDate);
        g_form.setValue('duration', daysToEnd);
    } else {
        // If current date is after end date, set days to 0 or handle as needed
        g_form.setValue('duration', 0);
    }
}

// Function to calculate difference in days between two dates
function dateDiffInDays(date1, date2) {
    var diffTime = Math.abs(date2 - date1);
    var diffDays = Math.ceil(diffTime / (1000 * 60 * 60 * 24));
    return diffDays;
}
]]></script>
