        <script>{"conditions":{"scriptActive":false,"script":"(function execute(inputs, outputs) {\n    var newBid \u003d inputs.bids[0]; // Get the new bid record from the trigger inputs\n\n    // Query for the item record related to the bid\n    var itemGR \u003d new GlideRecord(\u0027x_1383184_charityb_item\u0027);\n    if (itemGR.get(\u0027item_name\u0027, newBid.item_name)) {\n        // Query for the current highest bid\n        var highestBidGR \u003d new GlideRecord(\u0027x_1383184_charityb_bid\u0027);\n        highestBidGR.addQuery(\u0027item_name\u0027, itemGR.sys_id);\n        highestBidGR.orderByDesc(\u0027bid_amount\u0027);\n        highestBidGR.setLimit(1);\n        highestBidGR.query();\n\n        if (highestBidGR.next() \u0026\u0026 newBid.u_bid_amount \u003c highestBidGR.u_bid_amount) {\n            \n            var currentHighestBidder \u003d highestBidGR.bidder.getDisplayValue();\n            // Trigger an event for lower bid notification\n            gs.eventQueue(\u0027lower_bid_notification\u0027, newBid, currentHighestBidder, itemGR);\n\n            // Optionally log a message for confirmation\n            gs.info(\u0027Lower bid notification triggered for bidder: \u0027 + newBid.u_bidder.getDisplayValue());\n        }\n    }\n})(inputs, outputs);\n"}}</script>
