        <script><![CDATA[(function() {
    var eventItemGr = new GlideRecord('x_1383184_charityb_events');
    eventItemGr.query();

    while (eventItemGr.next()) {
        var startDate = new GlideDateTime(eventItemGr.start_date);
        var endDate = new GlideDateTime(eventItemGr.end_date);
        var currentDate = new GlideDateTime();

        if (currentDate.compareTo(startDate) >= 0 && currentDate.compareTo(endDate) < 0) {
            eventItemGr.event_status = 'In Progress';
        } else if (currentDate.compareTo(endDate) >= 0) {
            eventItemGr.event_status = 'Completed';
        }

        eventItemGr.update();
    }
})();]]></script>
